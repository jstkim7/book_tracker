<div id="dialog-form" title="Create new book">
  <%= form_for(:book, html: {multipart: true}) do |f| %>
  <% if @book.errors.any? %>
  <%= "errors." %>
  <% end %>
  <p>
    <%= f.label :title %></br>
    <%= f.text_field :title %>
  <p>
    <%= f.label :author %></br>
    <%= f.text_field :author %>
  </p>
  <p>
    <%= f.label :description %></br>
    <%= f.text_area :description, cols: '30', rows: '10' %>
  </p>
  <p>
    <%= f.label :pdf_book %>
    <%= f.file_field :pdf_book %>
  </p>
  <p>
    <%= f.button %>
  </p>
  <% end %>
</div>